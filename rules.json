[
  {
    "id": "test-rule-1",
    "version": "v0.40.2",
    "upstream": {
      "url": "http://host.docker.internal:3334"
    },
    "match": {
      "url": "http://<127.0.0.1|localhost>:4455/test",
      "methods": [
        "GET"
      ]
    },
    "authenticators": [
      {
        "handler": "oauth2_introspection"
      }
    ],

    "authorizer": {
      "handler": "remote_json",
      "config": {
        "remote" : "http://host.docker.internal:4466/relation-tuples/check?namespace=videos&object=/logs&relation=view&subject_id=c6397b7e-770d-48a5-9580-7af23fc152dc"
      }
    },
    "mutators": [
      {
        "handler": "header",
        "config": {
          "headers": {
            "X-Client-ID": "{{ print .Extra.client_id }}",
            "X-User": "",
            "Authorization": ""
          }
        }
      }
    ]
  }
]