[
  {
    "id": "test-rule-1",
    "version": "v0.40.2",
    "upstream": {
      "url": "http://host.docker.internal:3334"
    },
    "match": {
      "url": "http://<127.0.0.1|localhost>:4455/test",
      "methods": [
        "GET"
      ]
    },
    "authenticators": [
      {
        "handler": "oauth2_introspection"
      }
    ],
    "authorizer": {
      "handler": "keto_engine_acp_ory",
      "config": {
        "required_action": "check",
        "required_resource": "/test",
        "subject": "{{ print .Extra.client_id }}",
        "flavor": "exact"
      }
    },
    "mutators": [
      {
        "handler": "header",
        "config": {
          "headers": {
            "X-Client-ID": "{{ print .Extra.client_id }}",
            "X-User": "",
            "Authorization": ""
          }
        }
      }
    ]
  }
]