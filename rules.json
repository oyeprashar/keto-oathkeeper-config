[
  {
    "id": "test-rule-1",
    "version": "v0.40.2",
    "upstream": {
      "url": "http://host.docker.internal:3334"
    },
    "match": {
      "url": "http://<127.0.0.1|localhost>:4455/test",
      "methods": [
        "GET"
      ]
    },
    "authenticators": [
      {
        "handler": "oauth2_introspection"
      }
    ],
    "authorizer": {
      "handler": "allow"
    },
    "mutators": [
      {
        "handler": "header",
        "config": {
          "headers": {
            "X-Client-ID": "{{ print .Extra.client_id }}",
            "X-User": "",
            "Authorization": ""
          }
        }
      }
    ]
  }
]